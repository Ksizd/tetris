{
  "name": "tower-tetris-3d",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Браузерный 3D-тетрис на цилиндрической башне из кубиков.",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx}\" \"index.html\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\" \"index.html\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "tetris",
    "3d",
    "tower",
    "webgl",
    "game"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "@vitest/coverage-v8": "^4.0.14",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "vitest": "^4.0.14"
  },
  "dependencies": {
    "three": "^0.181.2"
  }
}
